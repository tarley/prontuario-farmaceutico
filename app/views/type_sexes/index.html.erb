<p id="notice"><%= notice %></p>

<h1>Listing Type Sexes</h1>

<table class='table'>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_sexes.each do |type_sex| %>
      <tr>
        <td><%= type_sex.description %></td>
        <td><%= link_to 'Show', type_sex %></td>
        <td><%= link_to 'Edit', edit_type_sex_path(type_sex) %></td>
        <td><%= link_to 'Destroy', type_sex, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

