<p id="notice"><%= notice %></p>

<h1>Listing Prms</h1>

<%= form_tag(prms_path, :method => :get) do %>
  <%= text_field_tag 'pesquisa', nil, placeholder: 'Buscar por Prms' %>
  <%= submit_tag "Pesquisar" , :class => 'btn btn-primary'%>
<% end %>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prms.each do |prm| %>
      <tr>
        <td><%= prm.description %></td>
        <td><%= link_to 'Show', prm %></td>
        <td><%= link_to 'Edit', edit_prm_path(prm) %></td>
        <td><%= link_to 'Destroy', prm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prm', new_prm_path %>
