<p id="notice"><%= notice %></p>

<h1>Listing Service Accesses</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_accesses.each do |service_access| %>
      <tr>
        <td><%= service_access.description %></td>
        <td><%= link_to 'Show', service_access %></td>
        <td><%= link_to 'Edit', edit_service_access_path(service_access) %></td>
        <td><%= link_to 'Destroy', service_access, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service access', new_service_access_path %>
